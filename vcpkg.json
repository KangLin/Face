{
  "name": "facerecognizer",
  "version-string":"0.0.4",
  "license": "GPL-3.0",
  "homepage": "https://github.com/KangLin/facerecognizer",

  "dependencies": [
    "openssl"
  ],
  
  "features": {
      "seetaface": {
          "description": "seetaface",
          "dependencies": [
              {
                  "name": "opencv",
                  "features": [
                      "ffmpeg"
                  ]
              }
          ]
      },
      "opencv": {
          "description": "opencv",
          "dependencies": [
              {
                  "name": "opencv",
                  "default-features": true,
                  "features": [
                      "contrib",
                      "ffmpeg"
                  ]
              }
          ]
      },
      "dlib": {
          "description": "dlib",
          "dependencies": [
              "dlib"
          ]
      }
  }
}
